<script src="{{ 'main-product.js' | asset_url }}" defer="defer"></script>

<style>
  .page-title {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media (max-width: 767px) {
    .page-title {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
</style>

<div class="layout_block_filter"></div>
<div class="loader_filter"></div>
<section
  id="MainProduct-{{ section.id }}"
  class="main-content"
  data-section="{{ section.id }}">
  <div class="page-title section-{{ section.id }}-padding gradient">
    <div class="product-main">
      <div
        class="product-cart-img"
        {% if section.settings.enable_sticky_img %}
        style="position: sticky; top: 150px;"
        {% endif %}>
        <ul class="product-img-thumb">
          {% for image in product.images %}
            <li class="product_slider_main {% if image == featured %}selected{% endif %}" id="product-featured-img-inner"><img
                class="image_slider_one"
                src="{{ image | img_url: 'master' }}"
                alt=""></li>
          {% endfor %}
        </ul>

        {% assign featured = product.selected_or_first_available_variant.featured_image %}
        <ul class="product-featured-img">
          {% for image in product.images %}
            <li class="image_slider" id="product-img-inner">
              <img
                class="image_slider_two"
                id="product-img"
                src="{{ image | image_url }}"
                alt="">
              <div class="zoom_effect">
                <svg
                  class="zoom_icon"
                  viewBox="0 -0.5 21 21"
                  version="1.1"
                  xmlns="http://www.w3.org/2000/svg"
                  xmlns:xlink="http://www.w3.org/1999/xlink"
                  fill="#ffffff">
