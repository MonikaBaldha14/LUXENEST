                  </g>
                </g>
              </g>
            </g>
          </svg>
        </div>
        {% for tag in product.tags %}
          {% if tag contains 'Rings' %}
            <div class="product-block_new-tag"><span>New</span></div>
          {% endif %}
        {% endfor %}
      </li>
    {% endfor %}
  </ul>
</div>
<div class="product-inner-main">
  {%- for block in section.blocks -%}
    {%- case block.type -%}
      {% when 'title' %}
        <div class="product-title">
          <h3>{{ product.title }}</h3>
        </div>
       

      {% when 'price' %}
        <div class="product-price">
          {% if product.compare_at_price != blank %}
            <p class="compare_at_price">{{ product.selected_or_first_available_variant.compare_at_price | money }}</p>
            <p class="price">{{ product.selected_or_first_available_variant.price | money }}</p>
          {% else %}
            <p class="price">{{ product.selected_or_first_available_variant.price | money }}</p>
          {% endif %}
        </div>

      {% when 'instock_notification' %}
        {% render 'instock-notification'
          , SuccessMsg: block.settings.success_msg
          , ErrorMsg: block.settings.error_msg %}

        {% when 'product-bundle' %}
          {% render 'product-bundle-available' %}
        

      {% when 'description' %}
        <div class="product-description">
          {{ product.description }}
        </div>

      {% when 'quantity_selector' %}
        {%- form 'product'
          , product
          , id: 'product_form_installment_id'
          , class: 'product-page-form' -%}
          <input
            type="hidden"
            name="return_to"
            value="/cart">
          <input
            type="hidden"
            id="product-id"
            name="id"
            value="{{ product.selected_or_first_available_variant.id }}">
          <div class="quanitity-main">
            <div class="quantity-selector">
              <button type="button" class="quantity-selector-minus quantity minus">
                <i class="fa-solid fa-minus quantity_icon minus-icon"></i>
              </button>
              <input
                id="quantity"
                name="quantity"
                value="1"
                class="product-quantity"
                readonly>
              <button type="button" class="quantity-selector-plus quantity plus">
                <i class="fa-solid fa-plus quantity_icon plus-icon"></i>
              </button>
            </div>
          </div>
        {% endform %}
      {% when 'cart_button' %}
        {%- form 'product'
          , product
          , id: 'product_form_installment_id'
          , class: 'product-page-form' -%}
          <input
            type="hidden"
            name="return_to"
            value="/cart">
          <input
            type="hidden"
            id="product-id"
            name="id"
            value="{{ product.selected_or_first_available_variant.id }}">
          <div class="product-cart">
            <button
              {% unless product.selected_or_first_available_variant.available %}
              disabled
              {% endunless %}
              id="add-to-cart-btn sticky-atc"
              class="product-add">
              {% if product.selected_or_first_available_variant.available %}
                Add to Cart
              {% else %}
                Out of Stock
              {% endif %}
            </button>

            <button
              {% unless product.selected_or_first_available_variant.available %}
              disabled
              {% endunless %}
              id="buy-it-now"
              type="submit"
              class="product-add product-buy">
              {% if product.selected_or_first_available_variant.available %}
                Buy It Now
              {% else %}
                <style>
                  .product-add.product-buy {
                    display: none;
                  }
                </style>
              {% endif %}
            </button>
            <p class="error_msg"></p>
          </div>
        {% endform %}

      {% when 'variant_picker' %}
        {%- form 'product'
          , product
          , id: 'product_form_installment_id'
          , class: 'product-page-form' -%}
          <input
            type="hidden"
            name="return_to"
            value="/cart">
          <input
            type="hidden"
            id="product-id"
            name="id"
            value="{{ product.selected_or_first_available_variant.id }}">
          {% assign current_variant = product.selected_or_first_available_variant %}
          <div class="quantity_val" value="{{ current_variant.inventory_quantity }}">
            {{ current_variant.inventory_quantity }}
          </div>
          {% if product.variants.size == 1 or product.variants.size > 1 %}
            <div class="product-option">
              {% for option in product.options_with_values %}
                <fieldset class="product-variants">
                  <legend class="product_variant-title">
                    {{ option.name }}
                  </legend>
                  <div class="variants-filter">
                    {% for value in option.values %}
                      <div class="option-value">
                        {% if option.name != blank or value != blank %}
                          {% if option.name == 'Color' %}
                            <input
                              {% if option.selected_value == value %}
                              checked{% endif %}
                              type="radio"
                              name="{{ option.name }}"
                              value="{{ value }}"
                              id="{{ option.name | handleize }}-{{ value | handleize }}"
                              class="product_variant_color product-variant-option">

                            {% liquid
                              if value
                                assign label = value.name
                                assign value = value.id
                              endif
                              assign downcase_label = label | downcase | strip
                            %}
                            {% liquid
                              if swatch.color != blank
                                assign swatch_style = 'background-image: linear-gradient(to right, ' | append: swatch.color | append: ', ' | append: swatch.color | append: ')'
                              else
                                assign swatch_config = settings.color_swatch_config | newline_to_br | split: '<br />'
                                assign swatch_style = 'background-image: linear-gradient(to right, ' | append: downcase_label | append: ', ' | append: downcase_label | append: ')'

                                for swatch_item in swatch_config
                                  assign swatch_parts = swatch_item | split: ':'
                                  assign swatch_name = swatch_parts.first | downcase | strip

                                  if downcase_label == swatch_name
                                    assign swatch_value = swatch_parts.last | strip

                                    if swatch_value contains '#'
                                      assign swatch_style = 'background-image: linear-gradient(to right, ' | append: swatch_value | append: ', ' | append: swatch_value | append: ')'
                                    elsif swatch_value contains 'linear-gradient('
                                      assign swatch_style = 'background-image: ' | append: swatch_value
                                    elsif images[swatch_value] != blank
                                      assign swatch_image = images[swatch_value] | image_url: width: 72
                                      assign swatch_style = 'background-color: url(' | append: swatch_image | append: ')'
                                    endif
                                    break
                                  endif
                                endfor
                              endif
                            %}
                            <label
                              style="{{ swatch_style }}; {% if value contains 'White' %} border: 1px solid #d7d7d7 {% endif %}"
                              class="variant-option-label-color"
                              for="{{ option.name | handleize }}-{{ value |  handleize }}"></label
                              >

                            <div class="product-variant-value">
                              <p>{{ value }}</p>
                            </div>
                          {% else %}
                            <input
                              {% if option.selected_value == value %}
                              checked{% endif %}
                              type="radio"
                              class="product-variant-option"
                              name="{{ option.name }}"
                              value="{{ value }}"
                              id="{{ option.name | handleize }}-{{ value |  handleize }}">
                            <label class="variant-option-label" for="{{ option.name | handleize }}-{{ value |  handleize }}">
                              <p>{{ value }}</p>
                            </label>
                          {% endif %}
                        {% endif %}
                      </div>
                    {% endfor %}
                  </div>
                </fieldset>
              {% endfor %}
            </div>
          {% else %}
            {% for option in product.options_with_values %}
              <input
                {% if option.selected_value == value %}
                checked{% endif %}
                type="radio"
                class="product-variant-option"
                name="{{ option.name }}"
                value="{{ value }}"
                id="{{ option.name | handleize }}-{{ value |  handleize }}">
              <label class="variant-option-label" for="{{ option.name | handleize }}-{{ value |  handleize }}">
                <p>{{ value }}</p>
              </label>
            {% endfor %}
          {% endif %}
        {% endform %}

      {% when 'comparison_product' %}
        {% render 'metafield-product',
        mainHeading: block.settings.heading,
        mainContent: block.settings.content-inner,
        secondImage: block.settings.second_img %}

      {% when 'collapsible_text' %}
        <div class="wrapper">
          {% if block.settings.title_one != blank or block.settings.description_one != blank %}
            <div class="container">
              {% if block.settings.title_one %}
                <div class="question">
                  <h4>{{ block.settings.title_one }}</h4>
                </div>
              {% endif %}
              {% if block.settings.description_one != blank %}
                <div class="answercont">
                  <div class="answer">
                    <p>{{ block.settings.description_one }}</p>
                  </div>
                </div>
              {% endif %}
            </div>
          {% endif %}

          {% if block.settings.title_two != blank or block.settings.description_two != blank %}
            <div class="container">
              {% if block.settings.title_two %}
                <div class="question">
                  <h4>{{ block.settings.title_two }}</h4>
                </div>
              {% endif %}
              {% if block.settings.description_two %}
                <div class="answercont">
                  <div class="answer">
                    <p>{{ block.settings.description_two }}</p>
                  </div>
                </div>
              {% endif %}
            </div>
          {% endif %}

          {% if block.settings.title_three != blank or block.settings.description_three != blank %}
            <div class="container">
              {% if block.settings.title_three %}
                <div class="question">
                  <h4>{{ block.settings.title_three }}</h4>
                </div>
              {% endif %}
              {% if block.settings.description_three %}
                <div class="answercont">
                  <div class="answer">
                    <p>{{ block.settings.description_three }}</p>
                  </div>
                </div>
              {% endif %}
            </div>
          {% endif %}

          {% if block.settings.title_four != blank or block.settings.description_four != blank %}
            <div class="container">
              {% if block.settings.title_four %}
                <div class="question">
                  <h4>{{ block.settings.title_four }}</h4>
                </div>
              {% endif %}
              {% if block.settings.description_four %}
                <div class="answercont">
                  <div class="answer">
                    <p>{{ block.settings.description_four }}</p>
                  </div>
                </div>
              {% endif %}
            </div>
          {% endif %}

          {% if block.settings.title_five != blank or block.settings.description_five != blank %}
            <div class="container">
              {% if block.settings.title_five %}
                <div class="question">
                  <h4>{{ block.settings.title_five }}</h4>
                </div>
              {% endif %}
              {% if block.settings.description_five %}
                <div class="answercont">
                  <div class="answer">
                    <p>{{ block.settings.description_five }}</p>
                  </div>
                </div>
              {% endif %}
            </div>
          {% endif %}
        </div>

      {% when 'icon' %}
        <div class="img_with_txt">
          <div class="img_with_txt_inner">
            <div class="icon_section">
              {% render 'icon-accordion'
                , icon: block.settings.icon %}
            </div>
            <h4 class="richtext_class">{{ block.settings.text_content }}</h4>
          </div>

          <div class="img_with_txt_inner">
            <div class="icon_section">
              {% render 'icon-accordion'
                , icon: block.settings.icon_two %}
            </div>
            <h4 class="richtext_class">{{ block.settings.text_content_two }}</h4>
          </div>

          <div class="img_with_txt_inner">
            <div class="icon_section">
              {% render 'icon-accordion'
                , icon: block.settings.icon_three %}
            </div>
            <h4 class="richtext_class">{{ block.settings.text_content_three }}</h4>
          </div>
        </div>
    {%- endcase -%}
  {%- endfor -%}

</div></div>
<div class="product_show">
<div class="img-show">
  <span class="close">
    <svg
      class="zooming_close"
      viewBox="0 0 24 24"
      fill="none"
      xmlns="http://www.w3.org/2000/svg">
      <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
      <g
        id="SVGRepo_tracerCarrier"
        stroke-linecap="round"
        stroke-linejoin="round"></g>
      <g id="SVGRepo_iconCarrier">
        <rect
          width="24"
          height="24"
          fill="transparent"></rect>
        <path
          d="M7 17L16.8995 7.10051"
          stroke="#000000"
          stroke-linecap="round"
          stroke-linejoin="round"></path>
        <path
          d="M7 7.00001L16.8995 16.8995"
          stroke="#000000"
          stroke-linecap="round"
          stroke-linejoin="round"></path>
      </g>
    </svg>
  </span>
  <img src="">
</div></div></div>{% render 'sticky-add-to-cart' %}</section>



<script>
      var product = {{ product | json }}
</script><script>
    // Change Variants JS

    document.querySelectorAll('.product-option input[type="radio"]').forEach(radio => {
    radio.addEventListener('change', () => {

        var selectedOption = []

        document.querySelectorAll('.product-option input[type="radio"]:checked').forEach(radio => {
            selectedOption.push(radio.value)
        })

        var matchedVariant = product.variants.find(variant => {
            var pass = true
            for (var i = 0; i < selectedOption.length; i++) {
                if (selectedOption.indexOf(variant.options[i]) === -1) {
                    pass = false
                    break
                }
            }
            return pass
        })

        document.querySelector("#product-id").value = matchedVariant.id;


        if(matchedVariant.compare_at_price){
          document.querySelector(".compare_at_price").textContent = formatMoney(matchedVariant.compare_at_price, "{{ shop.money_format }}");
          document.querySelector(".price").textContent = formatMoney(matchedVariant.price, "{{ shop.money_format }}");
        }

        if(matchedVariant.price){
          document.querySelector(".price").textContent = formatMoney(matchedVariant.price, "{{ shop.money_format }}");
        }
        
          document.querySelector('.slick-slide.slick-current.slick-active li#product-img-inner img#product-img').setAttribute('src', matchedVariant.featured_image.src)

        var add = document.querySelector('#add-to-cart-btn');
        var buyNow = document.querySelector("#buy-it-now");
        
        if (matchedVariant.available) {
            add.textContent = "Add to Cart"
            add.disabled = false
            buyNow.classList.remove("hide");
        }
        else {
            add.textContent = "Out of Stock"
            add.disabled = true
            buyNow.classList.add("hide");
        }
    })
})
</script>{% schema %}
{
