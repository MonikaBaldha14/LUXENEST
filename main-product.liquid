                    stroke-width="1"
                    fill="none"
                    fill-rule="evenodd"> <g
                    id="Dribbble-Light-Preview"
                    transform="translate(-379.000000, -440.000000)"
                    fill="#ffffff">
                  <g id="icons" transform="translate(56.000000, 160.000000)">
